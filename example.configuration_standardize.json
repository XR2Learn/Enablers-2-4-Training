{
  "dataset_config": {
    "dataset_name": "RAVDESS",
    "number_of_labels": 8
  },
  "pre_processing_config": {
    "process": "standardize",
    "create_splits": true,
    "target_sr": 16000,
    "padding": true,
    "max_length": 5
  },
  "encoder_config": {
    "from_module": "encoders.cnn1d",
    "class_name": "CNN1D",
    "kwargs": {
      "in_channels": 1,
      "len_seq": 80000,
      "out_channels": [
        2,
        2,
        2
      ],
      "kernel_sizes": [
        3,
        3,
        3
      ],
      "stride": 2
    }
  },
  "ssl_config": {
    "from_module": "ssl_methods.VICReg",
    "ssl_framework": "VICReg",
    "input_type": "standardize",
    "save_last_encoder": false,
    "epochs": 20,
    "batch_size": 32,
    "kwargs": {
      "lr": 0.0001,
      "n_views": 2,
      "temperature": 0.05,
      "optimizer_name_ssl": "adam"
    }
  },
  "sup_config": {
    "input_type": "standardize",
    "epochs": 20,
    "batch_size": 32,
    "use_augmentations_in_sup": true,
    "kwargs": {
      "lr": 0.0001,
      "optimizer_name": "adam",
      "freeze_encoder": true
    }
  },
  "augmentations": {
    "gaussian_noise": {
      "probability": 0.33,
      "kwargs": {
        "mean": 0,
        "std": 0.2
      }
    },
    "scale": {
      "probability": 0.4,
      "kwargs": {
        "max_scale": 1.3
      }
    }
  },
  "transforms": [
    {
      "class_name": "ToTensor",
      "from_module": "general_transforms",
      "transform_name": "to_tensor",
      "in_test": true
    },
    {
      "class_name": "Permute",
      "from_module": "general_transforms",
      "transform_name": "permutation",
      "in_test": true,
      "kwargs": {
        "shape": [
          1,
          0
        ]
      }
    },
    {
      "class_name": "ToFloat",
      "from_module": "general_transforms",
      "transform_name": "to_float",
      "in_test": true
    }
  ]
}